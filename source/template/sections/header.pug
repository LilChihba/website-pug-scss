- var menu1 = ['HOME', 'ABOUT', 'GALLERY']
- var menu2 = ['FAQ', 'FIND US', 'CONTACT']
header.header
    img.image__hover__header(src="images/Hover.png")
    nav.nav
        each menu in menu1
            a.nav__link(href="#")=menu
        img.logo__picture(src="images/InkTattooLogo.png" alt="picture")
        each menu in menu2
            a.nav__link(href="#")=menu